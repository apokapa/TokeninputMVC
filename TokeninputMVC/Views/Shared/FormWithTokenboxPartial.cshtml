@model TokeninputMVC.ViewModels.FormWithTokenboxVM

@using (Html.BeginForm("Index", "Results", FormMethod.Get, new { id = "myForm" }))
{
<div class="form-horizontal" style="padding-left:40px;padding-top:55px;padding-bottom:55px;">
    <div class="form-group">
        <div class="col-md-10">
            <h5>Type locations you are searching for</h5>
            @Html.EditorFor(m => m.tokenbox, new { @id = "tokenbox", htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-4 col-md-10">
            <input id="BTN" type="submit" value="Find" class="btn btn-primary" />
        </div>
    </div>
</div>
}